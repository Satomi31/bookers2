<%= render partial: 'books/info', locals: { user: @user, book_new: @book_new } %>

<h2>Users</h2>
<table>
  <thead>
    <tr>
      <th></th>
      <th>image</th>
      <th>name</th>
      <th></th>
    <tr>
  </thead>
  <tbody>
    <tr>
      <% @users.each do |user| %>
        <td>
          <%= image_tag user.get_profile_image %>
        </td>
        <td><%= user.name %></td>
        <td><%= link_to 'Show', user_path(user.id) %></td>
    </tr>
    <% end %>
  </tbody>
</table>